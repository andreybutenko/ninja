<main class="page-main page-listing section wide">
  <h3 class="thin">Andrey's <%= page.tag %> experiences</h3>
  <% page.posts.data
    .sort(function(a, b) {
      if(a.categories.data[0].name == 'Experience') {
        return -1;
      }
      else if (b.categories.data[0].name == 'Experience') {
        return 1;
      }

      return b.priority - a.priority;
    })
    .forEach(function(post){ %>
      <%- partial('_partials/ribbon', {post: post, index: true}) %>
  <% }) %>

  <div class="ribbon ribbon-blank">
    <div class="ribbon-body thin">

    </div>
  </div>
</main>
